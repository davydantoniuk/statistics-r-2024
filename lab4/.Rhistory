library(stats)
a <- 0.6
beta <- 1000
# (a) Średni czas awarii
mean_time <- beta * gamma(1 + 1/a)
print(paste("Średni czas awarii:", mean_time, "godzin"))
# (b) Odchylenie standardowe
sd_time <- sqrt(beta^2 * (gamma(1 + 2/a) - (gamma(1 + 1/a))^2))
print(paste("Odchylenie standardowe czasu awarii:", sd_time, "godzin"))
